{
	"name": "typescript-boilerplate",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"start": "webpack-dev-server --content-base dist",
		"build": "rm -rf dist && webpack -p",
		"deploy": "git add -f dist && git commit -m 'deploy' && git push origin `git subtree split --prefix dist master`:gh-pages --force && git rm -r dist && git commit -m 'cleanup deploy'",
		"release": "npm run build && npm run deploy"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"charwise": "^3.0.1",
		"cuid": "^2.1.4",
		"glamor": "^2.20.40",
		"levelup": "^3.1.1",
		"localstorage-down": "^0.6.7",
		"lodash": "^4.17.11",
		"react": "^16.7.0-alpha.0",
		"react-dom": "^16.7.0-alpha.0"
	},
	"devDependencies": {
		"@types/levelup": "^3.1.0",
		"@types/lodash": "^4.14.118",
		"@types/react": "^16.7.13",
		"@types/react-dom": "^16.0.11",
		"@types/webpack": "^4.4.20",
		"awesome-typescript-loader": "^5.2.1",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"ts-node": "^7.0.1",
		"typescript": "^3.2.2",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	}
}
